<center>
  <div class="panel panel-primary question">
    <div class="panel-heading">
      <h2 class="panel-tiile"><%=@question.question%></h2>
    </div>
    <div class="panel-body list-group">
      <%@question.variants.each do |v|%>
      <a class="pointer list-group-item variant"><h4><%=v.text%></h4></a>
      <%end%>
    </div>
  </div>
  <div class="panel panel-primary list-group">
    <a class="pointer list-group-item"><h4>Done!</h4></a>
  </div>
</center>
